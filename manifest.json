{
  "manifest_version": 2,
  "name": "Qiita Slide Fontsize Changer",
  "version": "0.0.1",
  "description": "Qiita Slide mode fontsize Changer",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://qiita.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://qiita.com/*"],
      "js": ["js/content_script.js"],
      "run_at": "document_end"
    }
  ]
}
